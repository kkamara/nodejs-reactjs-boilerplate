  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Backend",
      "request": "launch",
      "runtimeArgs": [
        "dev"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Frontend",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/frontend"
    }
  ]
}